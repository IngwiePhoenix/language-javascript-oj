      '0':
        'name': 'punctuation.section.scope.end.js.oj'
    'name': 'meta.bracketed.js.oj'
    'patterns': [
      {
        'begin': '(?=\\w)(?<=[\\w\\])"] )(\\w+(?:(:)|(?=\\])))'
        'beginCaptures':
          '1':
            'name': 'support.function.any-method.js.oj'
          '2':
            'name': 'punctuation.separator.arguments.js.oj'
        'end': '(?=\\])'
        'name': 'meta.function-call.js.oj'
        'patterns': [
          {
            'captures':
              '1':
                'name': 'punctuation.separator.arguments.js.oj'
            'match': '\\b\\w+(:)'
