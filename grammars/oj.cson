    'begin': '((@)(implementation))\\s+([A-Za-z][A-Za-z0-9]*)\\s*(?::\\s*([A-Za-z][A-Za-z0-9]*))?'
    'captures':
      '1':
        'name': 'storage.type.js.oj'
      '2':
        'name': 'punctuation.definition.storage.type.js.oj'
      '4':
        'name': 'entity.name.type.js.oj'
      '5':
